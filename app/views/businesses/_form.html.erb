<%= form_for @business do |f| %>
    <%= f.label :business_name %>
    <%= f.text_field :business_name %><br>
    <%= f.label :business_type %>
    <%= f.text_field :business_type %><br>
    <%= f.label :business_address %>
    <%= f.text_field :business_address %><br>
    <%= f.label :business_phone_number %>
    <%= f.text_field :business_phone_number %><br>
    <%= f.submit %>
  
  <% end %>