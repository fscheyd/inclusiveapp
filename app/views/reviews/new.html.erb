<%= form_for [@business, @review] do |f| %> 
  
  <%= f.hidden_field :business_id, value: @business.id %>
    <%= f.hidden_field :user_id, value: current_user.id %> 
  <%= f.label :review_title %>
  <%= f.text_field :review_title %><br>
  <%= f.label :accesibility_rating %>
  <%= f.text_field :accesibility_rating %><br>
  <%= f.label :review_body %>
  <%= f.text_field :review_body %><br>
  <%= f.label :accesibility_features %>
  <%= f.text_field :accesibility_features %><br>
  <%= f.submit %>
  
<% end %> 