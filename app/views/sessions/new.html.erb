<h1>Login Here!</h1>
<% if @user && @user.errors.any? %>

    <ul>
    <% @user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
    <% end %>
    </ul>
<% end %>
<%= form_for :user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br><br>
  <p>
  <%= f.label :password %>
  <%= f.password_field :password %><br><br>
  <p>
  <%= f.submit "login" %>

  
<% end %>