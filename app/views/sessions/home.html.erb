<h1>INCLUSIVE</h1>
<h2> Log in or Sign up to Review local businesses for Disability Accesibility!</h2>
<h3> <%= link_to "Log in", login_path%></h3>
<div>
<%= form_for @user, controller: 'users', action 'create' do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br><br>
    <%= f.label :email %><br>
    <%= f.email_field :email %><br><br>
    <%= f.label :password%><br>
    <%= f.password_field :password%><br><br>
    <%= f.submit "create"%>
<% end %>
</div>